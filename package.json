{
  "name": "webbybot",
  "version": "0.1.6",
  "description": "A simple helpful robot written in es6, compatible with hubot",
  "main": "dist/index.js",
  "bin": {
    "webby": "./bin/webby"
  },
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "babel index.js -o dist/index.js && babel src -d dist/src",
    "postbuild": "cp package.json dist/",
    "pretest": "source script/bootstrap",
    "test": "mocha --compilers js:babel-register -w",
    "test:single": "mocha --compilers js:babel-register",
    "lint": "eslint index.js bin/webby.js src/ test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gasolin/webbybot.git"
  },
  "keywords": [
    "github",
    "hubot",
    "webby",
    "bot",
    "es6"
  ],
  "author": "webby team",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/gasolin/webbybot/issues"
  },
  "homepage": "https://github.com/gasolin/webbybot",
  "dependencies": {
    "async": "^1.5.2",
    "chalk": "^1.1.1",
    "cline": "^0.8.2",
    "coffee-script": "^1.10.0",
    "connect-multiparty": "^2.0.0",
    "express": "^3.21.2",
    "log": "^1.4.0",
    "optparse": "^1.0.5",
    "scoped-http-client": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.6.5",
    "chai": "^3.5.0",
    "eslint": "~2.2.0",
    "eslint-config-defaults": "^9.0.0",
    "hubot-mock-adapter": "^1.0.0",
    "lodash": "^4.6.1",
    "mocha": "^2.4.5",
    "mockery": "^1.4.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  }
}
